{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "project": ["tsconfig.json"]
  },
  "env": {
      "browser": true, 
      "node": true,
      "es6": true
  },
  "extends": [
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
        "warn",
        {
          "indent": "space",
          "endOfLine": "auto"
        }
    ],
    "indent": "off"
  }
}